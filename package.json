{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "build": "NODE_ENV=production npm run _clean && webpack -p",
    "postbuild": "cp ./assets/paper-background-1.jpg ./dist/",
    "INACTIVE_postbuild": "cp ./index.html ./assets/paper-background-1.jpg ./dist/",
    "_clean": "rimraf dist",
    "INACTIVE_start": "nodemon ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "INACTIVE_test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "INACTIVE_test:watch": "npm run test -- --watch",
    "INACTIVE_prod:server": "npm run build && cd dist && python -m SimpleHTTPServer"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.8",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.0"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-preset-stage-1": "^6.1.18",
    "chart.js": "^2.4.0",
    "css-loader": "^0.26.0",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "just.randomcolor": "^0.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^3.10.1",
    "moment": "^2.17.0",
    "node-env-file": "^0.1.8",
    "node-sass": "^3.13.0",
    "rainbowvis.js": "^1.0.1",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-count-to": "^0.5.4",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "react-scroll-box": "^0.3.3-beta",
    "react-table": "^5.3.2",
    "react-widgets": "^3.4.5",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "toastr": "^2.1.2",
    "url-loader": "^0.5.7"
  }
}
